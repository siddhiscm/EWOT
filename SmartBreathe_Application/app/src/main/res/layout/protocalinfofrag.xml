<?xml version="1.0" encoding="utf-8"?>

<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:background="@color/background_color"
    android:id="@+id/scrollView1"
    >


    <TableLayout
        android:id="@+id/tableLayout1"
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true">

        <TableRow
            android:id="@+id/tableRow1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:textAlignment="center">

            <TextView
                android:layout_width="59dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/times_new_roman"
                android:gravity="center"
                android:paddingTop="40dp"
                android:text="Basic Training Protocol Info"
                android:textColor="@color/text_color"
                android:textStyle="bold">

            </TextView>
        </TableRow>

        <TableRow

            android:id="@+id/tableRow4"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:paddingTop="20dp"
            android:textAlignment="gravity">

            <TextView
                android:layout_width="115dp"
                android:layout_height="wrap_content"
                android:background="@drawable/cell_shape"
                android:fontFamily="@font/times_new_roman"
                android:gravity="center"
                android:paddingLeft="50dp"
                android:text="@string/phase"
                android:textColor="@color/text_color"
                android:textStyle="bold">

            </TextView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/cell_shape"
                android:textColor="@color/text_color"
                android:paddingLeft="10dp"
                android:text="@string/altitude"
                android:gravity="center"
                android:paddingRight="10dp"
                android:fontFamily="@font/times_new_roman"
                android:textStyle="bold">

            </TextView>

            <TextView
                android:layout_width="145dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:background="@drawable/cell_shape"
                android:paddingLeft="15dp"
                android:textColor="@color/text_color"
                android:text="Activity"
                android:fontFamily="@font/times_new_roman"
                android:textStyle="bold">

            </TextView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:paddingRight="70sp"
                android:text="@string/time"
                android:fontFamily="@font/times_new_roman"
                android:background="@drawable/cell_shape"
                android:textColor="@color/text_color"
                android:textAlignment="center"
                android:textStyle="bold">

            </TextView>


        </TableRow>
        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:paddingTop="20dp"
            android:background="@drawable/cell_shape"
            android:textAlignment="center">

            <TextView
                android:layout_width="94dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingLeft="70dp"
                android:textColor="@color/text_color"
                android:text="Prep Time"
                android:fontFamily="@font/times_new_roman"
                android:textStyle="bold">

            </TextView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@color/text_color"
                android:paddingLeft="10dp"
                android:text="--"
                android:paddingRight="10dp"
                android:fontFamily="@font/times_new_roman"
                android:textStyle="bold">

            </TextView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingLeft="15dp"
                android:textColor="@color/text_color"
                android:text="--"
                android:textStyle="bold"
                android:fontFamily="@font/times_new_roman">

            </TextView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:paddingRight="70sp"
                android:textColor="@color/text_color"
                android:text="10 Secs"
                android:fontFamily="@font/times_new_roman"
                android:textStyle="bold">

            </TextView>
        </TableRow>
        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:paddingTop="20dp"
            android:background="@drawable/cell_shape"
            android:textAlignment="center">

            <TextView
                android:layout_width="94dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingLeft="70dp"
                android:textColor="@color/text_color"
                android:text="Warm Up"
                android:fontFamily="@font/times_new_roman"
                android:textStyle="bold">

            </TextView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@color/text_color"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:text="-O2"
                android:fontFamily="@font/times_new_roman"
                android:textStyle="bold">

            </TextView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingLeft="15dp"
                android:textColor="@color/text_color"
                android:text="@string/warm_des"
                android:textStyle="bold"
                android:fontFamily="@font/times_new_roman">

            </TextView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:paddingRight="70sp"
                android:textColor="@color/text_color"
                android:text="30 Sec"
                android:fontFamily="@font/times_new_roman"
                android:textStyle="bold">

            </TextView>
        </TableRow>
        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/cell_shape"
            android:gravity="center_horizontal"
            android:paddingTop="20dp"
            android:textAlignment="center">

            <TextView
                android:layout_width="94dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@color/text_color"
                android:paddingLeft="70sp"
                android:text="EPO"
                android:fontFamily="@font/times_new_roman"
                android:textStyle="bold">

            </TextView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@color/text_color"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:text="-O2"
                android:fontFamily="@font/times_new_roman"
                android:textStyle="bold">

            </TextView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingLeft="15dp"
                android:textColor="@color/text_color"
                android:text="On Spot Running"
                android:textStyle="bold"
                android:fontFamily="@font/times_new_roman">

            </TextView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:paddingRight="70sp"
                android:textColor="@color/text_color"
                android:text="30 Sec"
                android:fontFamily="@font/times_new_roman"
                android:textStyle="bold">

            </TextView>
        </TableRow>
        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:paddingTop="20dp"
            android:background="@drawable/cell_shape"
            android:textAlignment="center">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@color/text_color"
                android:paddingLeft="75dp"
                android:text="Oxygen Wash"
                android:fontFamily="@font/times_new_roman"
                android:textStyle="bold">

            </TextView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@color/text_color"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:text="+O2"
                android:textStyle="bold"
                android:fontFamily="@font/times_new_roman">

            </TextView>

            <TextView
                android:layout_width="145dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingLeft="15dp"
                android:textColor="@color/text_color"
                android:text="On Spot Running"
                android:textStyle="bold"
                android:fontFamily="@font/times_new_roman">

            </TextView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:paddingRight="70sp"
                android:text="30 Sec"
                android:textColor="@color/text_color"
                android:textAlignment="center"
                android:textStyle="bold"
                android:fontFamily="@font/times_new_roman">
            </TextView>
        </TableRow>
        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:paddingTop="20dp"
            android:background="@drawable/cell_shape"
            android:textAlignment="center">

            <TextView
                android:layout_width="94dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@color/text_color"
                android:paddingLeft="70dp"
                android:text="Cool Down"
                android:fontFamily="@font/times_new_roman"
                android:textStyle="bold">

            </TextView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@color/text_color"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:text="+O2"
                android:fontFamily="@font/times_new_roman"
                android:textStyle="bold">

            </TextView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:fontFamily="@font/times_new_roman"
                android:paddingLeft="15dp"
                android:textColor="@color/text_color"
                android:text="@string/cool_des"
                android:textStyle="bold">
            </TextView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:paddingRight="70sp"
                android:textColor="@color/text_color"
                android:text="30 Sec"
                android:fontFamily="@font/times_new_roman"
                android:textStyle="normal|bold">
            </TextView>
        </TableRow>
    </TableLayout>
</ScrollView>
